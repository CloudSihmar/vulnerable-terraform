{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
      "MyApi": {
        "Type": "AWS::ApiGatewayV2::Api",
        "Properties" : {
            "ApiKeySelectionExpression" : "$request.header.x-api-key",
            "CorsConfiguration" : {
                "AllowCredentials" : true,
                "AllowHeaders" : [ "sample-allow-header" ],
                "AllowMethods" : [ "sample-allow-methods" ],
                "AllowOrigins" : [ "sample-allow" ],
                "ExposeHeaders" : [ "sample-exopse-header" ],
                "MaxAge" : 18
            },
            "CredentialsArn" : "sample-credientials-arn",
            "Description" : "description value",
            "DisableExecuteApiEndpoint" : false,
            "FailOnWarnings" : false,
            "Name" : "sampleApiGatewayV2",
            "ProtocolType" : "HTTP",
            "RouteKey" : "sample-route-key",
            "RouteSelectionExpression" : "$request.method $request.path",
            "Tags" : [{"Key": "env", "Value": "test" }],
            "Target" : "valid-target",
            "Version" : "v1.0.0"
          }
        }
    }
}