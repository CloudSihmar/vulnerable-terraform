{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Creating Neptune Cluster Instance service",
    "Resources": {
      "SampleNeptuneClusterInstance": {
          "Type": "AWS::Neptune::DBInstance",
          "Properties" : {
                "AllowMajorVersionUpgrade" : false,
                "AutoMinorVersionUpgrade" : false,
                "AvailabilityZone" : "us-west-1",
                "DBClusterIdentifier" : "${aws_neptune_cluster.default.id}",
                "DBInstanceClass" : "db.r4.large",
                "DBParameterGroupName" : "sampleparameter",
                "DBSubnetGroupName" : "mysubnetgroup",
                "PreferredMaintenanceWindow" : "07:00-09:00",
                "Tags" : [ { "Key" : "name", "Value" : "sample" }, { "Key" : "env", "Value" : "test" } ]
            }
        }
    }
}